2,0|2,1|2,2

formual= n/3,nmod3 ; where if n/3 < 1 == 0
                            if n/3 < 2 == 1
                                if n/3 < 3 == 2

0/3,(0mod3)|1/3,(1mod3)|2/3,(2mod3)
-----------------------------------
3/3,(3mod3)|4/3,(4mod3)|5/3,(5mod3)
-----------------------------------
6/3,(6mod3)|7/3,(7mod3)|8/3,(8mod3)
